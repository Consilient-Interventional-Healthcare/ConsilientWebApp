# Environment variables for local GitHub Actions testing with act
# These variables simulate GitHub Environment Variables and Repository Variables

# Environment-specific variables (dev defaults)
AZURE_REGION=canadacentral
AZURE_RESOURCE_GROUP_NAME=consilient-resource-group

# Repository variables (shared configuration)
CAE_CREATE_NEW=false
CAE_USE_SHARED=true
CAE_SHARED_NAME=consilient-env
CAE_NAME_TEMPLATE=consilient-cae-{environment}

# Non-sensitive configuration variables (previously misclassified as secrets)
SQL_ADMIN_USERNAME=sqladmin
AZURE_SQL_SERVER_FQDN=your-server.database.windows.net
ACR_REGISTRY_URL=your-registry.azurecr.io

# Application image names
API_IMAGE_NAME=consilientapi
REACT_IMAGE_NAME=consilientwebapp2

# Container registry
CONTAINER_REGISTRY=ghcr.io

# Version variables
SQL_SERVER_VERSION=2022-latest
SCHEMASPY_VERSION=6.2.4
JDBC_DRIVER_VERSION=12.4.2.jre11
