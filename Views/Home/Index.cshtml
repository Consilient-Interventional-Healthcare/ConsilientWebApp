@using static ConsilientWebApp.ViewModels.HomeIndexViewModel
@model ConsilientWebApp.ViewModels.HomeIndexViewModel

@{
    ViewData["Title"] = "Consilient Web App";
}

<main>
    <div class="relative">
        <!-- Secondary navigation -->
        <header class="pt-6 pb-4 sm:pb-6">
            <div class="mx-auto flex max-w-7xl flex-wrap items-center gap-6 px-4 sm:flex-nowrap sm:px-6 lg:px-8">
                <h1 class="text-base/7 font-semibold text-brand-blue">Welcome to the Consilient Web App</h1>
                <div class="order-last flex w-full gap-x-8 text-sm/6 font-semibold sm:order-0 sm:w-auto sm:border-l sm:border-gray-200 sm:pl-6 sm:text-sm/7">
                    <a asp-controller="Home" asp-action="Index" asp-route-timeframe="@TimeframeOptions.Today" class="@(Model.SelectedTimeframe== TimeframeOptions.Today ? "text-brand-blue" : "text-gray-700")">Today</a>
                    <a asp-controller="Home" asp-action="Index" asp-route-timeframe="@TimeframeOptions.Last7Days" class="@(Model.SelectedTimeframe == TimeframeOptions.Last7Days ? "text-brand-blue" : "text-gray-700")">Last 7 days</a>
                    <a asp-controller="Home" asp-action="Index" asp-route-timeframe="@TimeframeOptions.Last30Days" class="@(Model.SelectedTimeframe == TimeframeOptions.Last30Days ? "text-brand-blue" : "text-gray-700")">Last 30 days</a>
                    <a asp-controller="Home" asp-action="Index" asp-route-timeframe="@TimeframeOptions.AllTime" class="@(Model.SelectedTimeframe == TimeframeOptions.AllTime ? "text-brand-blue" : "text-gray-700")">All Time</a>
                </div>
@*                 <a href="#" class="ml-auto flex items-center gap-x-1 rounded-md bg-brand-blue px-3 py-2 text-sm font-semibold text-white shadow-xs hover:bg-brand-blue-dark focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-brand-blue">
                    <svg viewBox="0 0 20 20" fill="currentColor" aria-hidden="true" class="-ml-1.5 size-5">
                        <path d="M10.75 6.75a.75.75 0 0 0-1.5 0v2.5h-2.5a.75.75 0 0 0 0 1.5h2.5v2.5a.75.75 0 0 0 1.5 0v-2.5h2.5a.75.75 0 0 0 0-1.5h-2.5v-2.5Z" />
                    </svg>
                    New
                </a> *@
            </div>
        </header>

        <!-- Stats -->
        <div class="border-b border-b-gray-900/10 lg:border-t lg:border-t-gray-900/5">
            <dl class="mx-auto grid max-w-7xl grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 lg:px-2 xl:px-0">
                <div class="flex flex-wrap items-baseline justify-between gap-x-4 gap-y-2 border-t border-gray-900/5 px-4 py-10 sm:px-6 lg:border-t-0 xl:px-8">
                    <dt class="text-sm/6 font-medium text-gray-500">Encounters</dt>
                    @* <dd class="text-xs font-medium text-brand-blue">+4</dd> *@
                    <dd class="w-full flex-none text-3xl/10 font-medium tracking-tight text-gray-900">@Model.EncountersToday</dd>
                </div>
                <div class="flex flex-wrap items-baseline justify-between gap-x-4 gap-y-2 border-t border-gray-900/5 px-4 py-10 sm:border-l sm:px-6 lg:border-t-0 xl:px-8">
                    <dt class="text-sm/6 font-medium text-gray-500">Pending Approvals</dt>
                    @* <dd class="text-xs font-medium text-brand-yellow">+2</dd> *@
                    <dd class="w-full flex-none text-3xl/10 font-medium tracking-tight text-brand-yellow">@Model.PendingApprovals</dd>
                </div>
                <div class="flex flex-wrap items-baseline justify-between gap-x-4 gap-y-2 border-t border-gray-900/5 px-4 py-10 sm:px-6 lg:border-t-0 lg:border-l xl:px-8">
                    <dt class="text-sm/6 font-medium text-gray-500">Approved</dt>
                    @* <dd class="text-xs font-medium text-gray-700">-1</dd> *@
                    <dd class="w-full flex-none text-3xl/10 font-medium tracking-tight text-green-700">@Model.ApprovedEncounters</dd>
                </div>
                <div class="flex flex-wrap items-baseline justify-between gap-x-4 gap-y-2 border-t border-gray-900/5 px-4 py-10 sm:border-l sm:px-6 lg:border-t-0 xl:px-8">
                    <dt class="text-sm/6 font-medium text-gray-500">Errors</dt>
                    @* <dd class="text-xs font-medium text-rose-600">0</dd> *@
                    <dd class="w-full flex-none text-3xl/10 font-medium tracking-tight text-rose-600">@Model.ErrorEncounters</dd>
                </div>
            </dl>
        </div>
    </div>


    <div class="space-y-16 py-16 xl:space-y-20">
        <!-- Modules grid (fixed to fill width) -->
        <div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8">
            <div class="mx-auto max-w-none lg:mx-0">
                <div class="grid grid-cols-[repeat(auto-fit,minmax(22rem,1fr))] gap-8">

                    <!-- Review Patient Encounters -->
                    <section class="w-full h-full overflow-hidden rounded-2xl bg-white ring-1 ring-gray-200 shadow-sm hover:shadow-md transition">
                        <div class="flex items-center gap-x-4 border-b border-gray-100 bg-gray-50 p-6">
                            <div class="flex size-12 items-center justify-center rounded-lg ring-1 ring-gray-900/10 bg-brand-blue/10">
                                <svg viewBox="0 0 24 24" aria-hidden="true" class="size-6 text-brand-blue">
                                    <path fill="currentColor" d="M9 3a2 2 0 0 0-2 2H6a3 3 0 0 0-3 3v10a3 3 0 0 0 3 3h12a3 3 0 0 0 3-3V8a3 3 0 0 0-3-3h-1a2 2 0 0 0-2-2H9Zm0 2h6v2H9V5Z" />
                                </svg>
                            </div>
                            <div class="text-sm/6 font-medium text-gray-900">Review Patient Encounters</div>
                            <button type="button" class="relative ml-auto text-gray-400 hover:text-gray-600" aria-label="Open options">
                                <svg viewBox="0 0 20 20" fill="currentColor" class="size-5"><path d="M3 10a1.5 1.5 0 1 1 3 0 1.5 1.5 0 0 1-3 0ZM8.5 10a1.5 1.5 0 1 1 3 0 1.5 1.5 0 0 1-3 0ZM15.5 8.5a1.5 1.5 0 1 0 0 3 1.5 1.5 0 0 0 0-3Z" /></svg>
                            </button>
                        </div>

                        <dl class="divide-y divide-gray-100 px-6 py-4 text-sm/6">
                            <div class="flex justify-between gap-x-4 py-3">
                                <dt class="text-gray-500">Overview</dt>
                                <dd class="text-gray-700">Review, upload, manage and approve</dd>
                            </div>
                            <div class="flex justify-between gap-x-4 py-3">
                                <dt class="text-gray-500">Last 7 days</dt>
                                <dd class="flex items-center gap-2">
                                    <span class="rounded-md bg-brand-yellow/10 px-2 py-0.5 text-xs font-medium text-brand-yellow ring-1 ring-inset ring-brand-yellow/20">@Model.Last7DaysPending</span>
                                </dd>
                            </div>
                        </dl>

                        <div class="border-t border-gray-100 p-6">
                            <a asp-controller="PatientVisitsStaging" asp-action="Index"
                               class="inline-flex items-center justify-center rounded-lg bg-brand-blue px-4 py-2.5 text-sm font-semibold text-white shadow-xs hover:bg-brand-blue-dark focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-brand-blue">
                                Patient Encounters
                            </a>
                        </div>
                    </section>

                    <!-- Approved Patient Encounters -->
                    <section class="w-full h-full overflow-hidden rounded-2xl bg-white ring-1 ring-gray-200 shadow-sm hover:shadow-md transition">
                        <div class="flex items-center gap-x-4 border-b border-gray-100 bg-gray-50 p-6">
                            <div class="flex size-12 items-center justify-center rounded-lg ring-1 ring-gray-900/10 bg-brand-blue/10">
                                <svg viewBox="0 0 24 24" aria-hidden="true" class="size-6 text-brand-blue">
                                    <path fill="currentColor" d="M12 2 9.9 7.1 4.5 7.8l4 3.7L7.6 17 12 14l4.4 3-1-5.5 4-3.7-5.4-.7z" />
                                </svg>
                            </div>
                            <div class="text-sm/6 font-medium text-gray-900">Approved Patient Encounters</div>
                            <button type="button" class="relative ml-auto text-gray-400 hover:text-gray-600" aria-label="Open options">
                                <svg viewBox="0 0 20 20" fill="currentColor" class="size-5"><path d="M3 10a1.5 1.5 0 1 1 3 0 1.5 1.5 0 0 1-3 0ZM8.5 10a1.5 1.5 0 1 1 3 0 1.5 1.5 0 0 1-3 0ZM15.5 8.5a1.5 1.5 0 1 0 0 3 1.5 1.5 0 0 0 0-3Z" /></svg>
                            </button>
                        </div>

                        <dl class="divide-y divide-gray-100 px-6 py-4 text-sm/6">
                            <div class="flex justify-between gap-x-4 py-3">
                                <dt class="text-gray-500">Status</dt>
                                @if (Model.PendingApprovals == 0)
                                {
                                    <dd class="flex items-center gap-2">
                                        <span class="rounded-md bg-green-50 px-2 py-0.5 text-xs font-medium text-green-700 ring-1 ring-inset ring-green-600/20">Up to date</span>
                                    </dd>
                                }
                                else if (Model.PendingApprovals > 0)
                                {
                                    <dd class="flex items-center gap-2">
                                        <span class="rounded-md bg-yellow-50 px-2 py-0.5 text-xs font-medium text-yellow-700 ring-1 ring-inset ring-yellow-600\/20">Approvals Needed</span>
                                    </dd>
                                }

                            </div>
                            <div class="flex justify-between gap-x-4 py-3">
                                <dt class="text-gray-500">Last 7 days</dt>
                                <dd class="text-gray-900 font-medium">@Model.Last7DaysApproved</dd>
                            </div>
                        </dl>

                        <div class="border-t border-gray-100 p-6">
                            <a asp-controller="PatientVisits" asp-action="Index"
                               class="inline-flex items-center justify-center rounded-lg bg-brand-blue px-4 py-2.5 text-sm font-semibold text-white shadow-xs hover:bg-brand-blue-dark focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-brand-blue">
                                Patient Encounters (Approved)
                            </a>
                        </div>
                    </section>

                    <!-- Administration (Admin only) -->
                    @if (User.IsInRole("Administrator"))
                    {
                        <section class="w-full h-full overflow-hidden rounded-2xl bg-white ring-1 ring-gray-200 shadow-sm hover:shadow-md transition">
                            <div class="flex items-center gap-x-4 border-b border-gray-100 bg-gray-50 p-6">
                                <div class="flex size-12 items-center justify-center rounded-lg ring-1 ring-gray-900/10 bg-brand-blue/10">
                                    <svg viewBox="0 0 24 24" aria-hidden="true" class="size-6 text-brand-blue">
                                        <path fill="currentColor" d="M19.14 12.94a7.97 7.97 0 0 0 0-1.88l2.03-1.58a.5.5 0 0 0 .12-.66l-1.92-3.32a.5.5 0 0 0-.61-.22l-2.39.96c-.57.23-1.11.53-1.62.94l-.36-2.54a.5.5 0 0 0-.5-.42H9.06a.5.5 0 0 0-.5.42l-.36 2.54a6.8 6.8 0 0 0-1.62.94l-2.39-.96a.5.5 0 0 0-.6.22L2.7 8.82a.5.5 0 0 0 .11.66l2.03 1.58c-.06.62-.06 1.25 0 1.88L2.8 14.52a.5.5 0 0 0-.11.66l1.92 3.32a.5.5 0 0 0 .6.22l2.39-.96c.5.4 1.05.72 1.62.94l.36 2.54a.5.5 0 0 0 .5.42h3.84a.5.5 0 0 0 .5-.42l.36-2.54c.57-.22 1.11-.53 1.62-.94l2.39.96a.5.5 0 0 0 .61-.22l1.92-3.32a.5.5 0 0 0-.12-.66l-2.03-1.58Z" />
                                    </svg>
                                </div>
                                <div class="text-sm/6 font-medium text-gray-900">Administration</div>
                            </div>

                            <ul class="-my-3 divide-y divide-gray-100 px-2 py-2 text-sm/6">
                                <li>
                                    <a asp-controller="Employees" asp-action="Index" class="flex items-center justify-between gap-4 rounded-lg px-4 py-3 text-brand-blue hover:bg-gray-50 focus:outline-none">
                                        <span>Employees</span>
                                        <svg viewBox="0 0 20 20" fill="currentColor" class="size-5 text-gray-400"><path d="M7.2 15.3a1 1 0 0 1 0-1.4l3.2-3.2-3.2-3.2a1 1 0 1 1 1.4-1.4l4 4a1 1 0 0 1 0 1.4l-4 4a1 1 0 0 1-1.4 0Z" /></svg>
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="ServiceTypes" asp-action="Index" class="flex items-center justify-between gap-4 rounded-lg px-4 py-3 text-brand-blue hover:bg-gray-50 focus:outline-none">
                                        <span>Service Types</span>
                                        <svg viewBox="0 0 20 20" fill="currentColor" class="size-5 text-gray-400"><path d="M7.2 15.3a1 1 0 0 1 0-1.4l3.2-3.2-3.2-3.2a1 1 0 1 1 1.4-1.4l4 4a1 1 0 0 1 0 1.4l-4 4a1 1 0 0 1-1.4 0Z" /></svg>
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="Facilities" asp-action="Index" class="flex items-center justify-between gap-4 rounded-lg px-4 py-3 text-brand-blue hover:bg-gray-50 focus:outline-none">
                                        <span>Facilities</span>
                                        <svg viewBox="0 0 20 20" fill="currentColor" class="size-5 text-gray-400"><path d="M7.2 15.3a1 1 0 0 1 0-1.4l3.2-3.2-3.2-3.2a1 1 0 1 1 1.4-1.4l4 4a1 1 0 0 1 0 1.4l-4 4a1 1 0 0 1-1.4 0Z" /></svg>
                                    </a>
                                </li>
                            </ul>

                            <div class="border-t border-gray-100 p-6">
                                <p class="text-xs text-gray-500">Manage employees, services, and other settings.</p>
                            </div>
                        </section>
                    }
                </div>
            </div>
        </div>


