{
  "profiles": {
    "LocalDev": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "https://localhost:8091",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "LocalDev"
      },
      "applicationUrl": "https://localhost:8091"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "https://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://+:8091",
        "ASPNETCORE_Kestrel__Certificates__Default__Path": "/certs/localhost.pem",
        "ASPNETCORE_Kestrel__Certificates__Default__KeyPath": "/certs/localhost-key.pem"
      },
      "dockerLaunchAction": "LaunchBrowser",
      "sslPort": 8091,
      "useSSL": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}