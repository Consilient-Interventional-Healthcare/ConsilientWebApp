{
  "profiles": {
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "https://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://+:8091",
        "ASPNETCORE_Kestrel__Certificates__Default__Path": "/https/consilient-api.pfx",
        "ASPNETCORE_Kestrel__Certificates__Default__Password": "DevCertPassword123!"
      },
      "dockerLaunchAction": "LaunchBrowser",
      "sslPort": 8091,
      "useSSL": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}