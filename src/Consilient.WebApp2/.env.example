# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api

# Application Environment
# Options: development, staging, production
VITE_APP_ENV=development

# Feature Flags (optional)
# VITE_ENABLE_ANALYTICS=true
# VITE_ENABLE_DEBUG_MODE=false

# OAuth/Authentication (if applicable)
# VITE_OAUTH_CLIENT_ID=your-client-id-here
# VITE_OAUTH_AUTHORITY=https://login.microsoftonline.com/your-tenant-id
# VITE_OAUTH_REDIRECT_URI=http://localhost:5173/auth/callback

# Logging Configuration (optional)
# Enable sending logs to backend API in development (default: false, always enabled in production)
# VITE_ENABLE_REMOTE_LOGGING=true
