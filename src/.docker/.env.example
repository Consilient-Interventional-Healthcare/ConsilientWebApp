# Docker Compose Environment Variables
# Copy this file to .env and fill in the values

# Database
SA_PASSWORD=YourStrong!Passw0rd

# Grafana
GRAFANA_ADMIN_PASSWORD=admin

# Ollama WebUI
OLLAMA_WEBUI_SECRET_KEY=your-secret-key-here

# Development HTTPS Certificate
# Generate with: dotnet dev-certs https -ep ~/.aspnet/https/consilient-api.pfx -p "DevCertPassword123!" --trust
DEV_CERT_PASSWORD=DevCertPassword123!

# Docker Registry (optional)
# DOCKER_REGISTRY=
